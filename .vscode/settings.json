{
  "editor.formatOnSave": true,
  "editor.formatOnPaste": true,
  "tailwindCSS.experimental.classRegex": [
    ["Classes \\=([^;]*);", "'([^']*)'"],
    ["Classes \\=([^;]*);", "\"([^\"]*)\""],
    ["Classes \\=([^;]*);", "\\`([^\\`]*)\\`"],
    ["Styles \\=([^;]*);", "'([^']*)'"],
    ["Styles \\=([^;]*);", "\"([^\"]*)\""],
    ["Styles \\=([^;]*);", "\\`([^\\`]*)\\`"]
  ],
  "prettier.configPath": "prettier.config.js"
}
